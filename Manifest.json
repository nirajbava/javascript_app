{
    "name": "Weather App",
    "version": "1.0",
    "description": "my weather app",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": "logo.png"
    },
    "permissions":[
        "geolocation",
        "activeTab",
        "https://*/",
        "unlimitedStorage",
        "activeTab"
    ],
    "content_scripts":[{
        "matches":["<all_urls>"],
        "js":["main.js"]
    }],
    "background": {
        "scripts": ["main.js"],
        "persistent": false
    }
}
